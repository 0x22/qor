<label class="label" for="{{.InputId}}">{{.Label}}</label>
{{$meta := .Meta}}
{{if .Value}}
    {{range $index, $value := .Value }}
      <fieldset>
          {{render_form $value (allowed_metas $meta.Resource) $index}}
      </fieldset>
    {{end}}
{{end}}

<fieldset>
  {{render_form $meta.Resource.GetResource.Value (allowed_metas $meta.Resource) (len .Value)}}
</fieldset>
