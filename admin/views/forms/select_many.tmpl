<div>
  <label class="label" for="{{.InputId}}">{{.Label}}</label><br/>
  <select multiple id="{{.InputId}}" name="{{.InputName}}">
    {{$current_values := .Value}}
    {{range $values := .CollectionValue}}
       {{if (has_primary_key $current_values (index $values 0))}}
         <option value="{{index $values 0}}" selected="selected">{{index $values 1}}</option>
       {{else}}
         <option value="{{index $values 0}}">{{index $values 1}}</option>
      {{end}}
    {{end}}
  </select>
</div>
