<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
  <select class="mdl-textfield__input" id="{{.InputId}}" data-toggle="qor.chooser" name="{{.InputName}}"{{if not (has_update_permission .Meta)}} disabled{{end}}>
    {{$current_values := .Value}}
    {{range $values := .CollectionValue}}
      {{if (is_included $current_values (index $values 0))}}
        <option value="{{index $values 0}}" selected>{{index $values 1}}</option>
      {{else}}
        <option value="{{index $values 0}}">{{index $values 1}}</option>
      {{end}}
    {{end}}
  </select>
  <label class="mdl-textfield__label" for="{{.InputId}}">{{dt (printf "%v.attributes.%v" .Base.ToParam .Label) .Label}}</label>
</div>

